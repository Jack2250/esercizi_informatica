public class Main {
    public static void main(String[] args) {
        String numero = "1234";
        int valChar = '0';
        /*System.out.println(valChar);
        char varChar=49;
        System.out.println(varChar);*/
        //Stampa caratteri stringa
        for (int i = 0; i < numero.length(); i++) {
            System.out.println(numero.charAt(i) + " carattere");
            System.out.println(numero.charAt(i) - 48 + " valore numerico");

        }

    }

}
